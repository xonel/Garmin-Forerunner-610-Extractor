[Desktop Entry]
Version=1.0
Type=Application
Terminal=false
Categories=GNOME;GTK;

# commande à exécuter lorsque le lanceur est pressé
Exec=xterm -font -*-fixed-medium-r-*-*-18-*-*-*-*-*-iso8859-* -geometry 75x40 -e 'cd $HOME/GFrun && sudo /bin/bash ./GFrun.sh'
# nom du lanceur
Name=GFrunS

# description plus élaborée du lanceur
Comment= GFrunS - Garmin ForeRunner (ROOT)

# icône à appliquer au lanceur
Icon=/usr/share/icons/GFrunS.svg
